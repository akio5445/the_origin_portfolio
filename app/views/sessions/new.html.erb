<h1>Sessions#new</h1>
<h1>ログイン</h1>
<%= form_with scope: :session, local: true do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in", class: "submit_button"%>
<% end %>

<%= link_to "新規登録", new_user_path, class: "button" %>